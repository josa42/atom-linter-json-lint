{
  "name": "linter-json-lint",
  "main": "./lib/init",
  "version": "0.1.2",
  "description": "Lint `json` on the fly, using json-lint",
  "repository": "https://github.com/josa42/atom-linter-json-lint",
  "license": "MIT",
  "scripts": {
    "lint": "standard",
    "lint:fix": "standard --fix"
  },
  "engines": {
    "atom": ">0.50.0"
  },
  "dependencies": {
    "atom-package-deps": "^4.0.1",
    "json-lint": "^0.1.0 "
  },
  "package-deps": [
    "linter"
  ],
  "providedServices": {
    "linter": {
      "versions": {
        "1.0.0": "provideLinter"
      }
    }
  },
  "devDependencies": {
    "standard": "^10.0.3"
  }
}
